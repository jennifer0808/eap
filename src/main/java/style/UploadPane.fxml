<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="550.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.tzauto.octopus.gui.dialog.uploadpane.UploadPaneController">
    <stylesheets>
        <URL value="@uploadpane.css" />
    </stylesheets>
    <children>
      <ProgressIndicator fx:id="progressIndicatorLoad" layoutX="240.0" layoutY="250.0" maxHeight="77.0" maxWidth="80.0" prefHeight="77.0" prefWidth="80.0" visible="false">
         <effect>
            <ColorAdjust brightness="-0.62" contrast="1.0" />
         </effect>
      </ProgressIndicator>
      <AnchorPane fx:id="mainPane" prefHeight="600.0" prefWidth="550.0">
         <children>
              <Button fx:id="BTN_cancel" layoutX="388.0" layoutY="566.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="80.0" style="-fx-background-color: #0069CC;" text="%cancel" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="14.0" layoutY="76.0" text="%deviceRecipeList">
               <font>
                  <Font size="11.0" />
               </font>
            </Label>
              <Label layoutX="78.0" layoutY="34.0" text="%deviceList" />
              <ComboBox fx:id="CMB_deviceCode" layoutX="154.0" layoutY="29.0" prefWidth="150.0" promptText="--请选择--" />
              <TableView fx:id="dataTable" layoutX="9.0" layoutY="91.0" onContextMenuRequested="#fillTabView" prefHeight="456.0" prefWidth="532.0">
                  <columns>
                  <TableColumn fx:id="saveCol" maxWidth="40.0" prefWidth="40.0" />
                      <TableColumn fx:id="numberCol" maxWidth="130.0" sortable="false" text="%number" />
                      <TableColumn fx:id="recipeCodeCol" maxWidth="184.0" prefWidth="133.0" text="%deviceCode" />
                      <TableColumn fx:id="recipeNameCol" maxWidth="368.0" prefWidth="275.0" text="%recipeName" />
                  </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
              </TableView>
              <Button fx:id="BTN_ok" layoutX="289.0" layoutY="567.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="80.0" style="-fx-background-color: #0069CC;" text="%ok" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Calibri Light" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
